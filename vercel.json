{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/ask.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index"
    },
    {
      "src": "/api/ask",
      "dest": "/api/ask"
    },
    {
      "src": "/api/test",
      "dest": "/api/test"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/debug",
      "dest": "/api/debug"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/ask.js": {
      "maxDuration": 30
    },
    "api/test.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 30
    },
    "api/debug.js": {
      "maxDuration": 30
    }
  }
} 